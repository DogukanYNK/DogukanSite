@page
@model DogukanSite.Pages.Account.RegisterModel
@{
    ViewData["Title"] = "Kayıt Ol";
}
<h2>Kayıt Ol</h2>
<form method="post">
    <input asp-for="Email" placeholder="Email" class="form-control" />
    <input asp-for="Password" type="password" placeholder="Şifre" class="form-control" />
    <button type="submit" class="btn btn-success">Kayıt Ol</button>
</form>
